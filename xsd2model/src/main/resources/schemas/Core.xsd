<?xml version="1.0" encoding="UTF-8"?>
<!-- Hint:
    if you want generate xml with a namespace then change 'unqualified' values to 'qualified',
    but you should be aware of https://bugs.openjdk.java.net/browse/JDK-8134111
-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://xsd2model.org/core"
           xmlns:ns="http://xsd2model.org/core"
>

    <xs:element name="userType" type="ns:UserType"/>

    <xs:complexType name="UserType">
        <xs:sequence>
            <xs:element type="xs:string" name="login"/>
            <xs:element type="xs:string" name="password"/>
        </xs:sequence>
    </xs:complexType>

</xs:schema>